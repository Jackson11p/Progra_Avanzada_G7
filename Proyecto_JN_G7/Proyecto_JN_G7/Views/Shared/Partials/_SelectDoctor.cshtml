@model IEnumerable<Proyecto_JN_G7.Models.DoctorListItem>
<select name="doctor" id="doctor" class="form-select" aria-label="Seleccione un médico">
    <option value="">Sin preferencia de médico</option>
    @foreach (var d in Model)
    {
        <option value="@d.NombreCompleto">@d.NombreCompleto — @d.Especialidad</option>
    }
</select>
